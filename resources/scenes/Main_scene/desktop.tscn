	# Max_9th

@onready var ambientsfx: AudioStreamPlayer = $ambientsfx
@onready var splash_screen: CanvasLayer = $splash_screen
@onready var background: TextureRect = $contents/background
@onready var time: Label = $contents/panel/Panel/time
@onready var date: Label = $contents/panel/Panel/date
@onready var sfx_button: Button = $contents/panel/Panel/sfx_button
@onready var sfx_button_sprite: TextureRect = $contents/panel/Panel/HBoxContainer/sfx_button_sprite
@onready var vfx: ColorRect = $vfx
@onready var vfx_button_sprite: TextureRect = $contents/panel/Panel/HBoxContainer/vfx_button_sprite
@onready var windows: Control = $contents/windows
@onready var easter_button: Button = $contents/panel/Panel/easter_button
@onready var easter_button_sprite: TextureRect = $contents/panel/Panel/HBoxContainer/easter_button_sprite
@onready var music: TextureButton = $contents/Folders/rightvboxcon/music
@onready var folders: Control = $contents/Folders
@onready var panel: Panel = $contents/panel/Panel
@onready var class_manager: Panel = $contents/windows/class_manager
@onready var os_billboard: Control = $contents/os_billboard

@onready var vfx_texture = preload(\"res://resources/sprites/vfx.png\")
@onready var disabled_vfx_texture = preload(\"res://resources/sprites/disabledvfx.png\")

@onready var default_bk_texture = preload(\"res://resources/sprites/bk.png\")

var textcolor: bool = true

signal stop
signal textcolorchange
signal welcome_display

var easter_state: bool = false

func _on_power_button_pressed() -> void:
	self.queue_free()
	get_tree().quit()

func _ready() -> void:
	vfx.visible = true
	vfx_button_sprite.texture = vfx_texture
	sfx_button_sprite.texture = load(\"res://resources/sprites/sfx.png\")
	easter_button_sprite.visible = false
	easter_button.disabled = true
	windows.position.x = 0
	update_time()
	if JavaScriptBridge.eval(\"checkVisit();\"):
		var result = JavaScriptBridge.eval(\"checkVisit();\")
		if result:
			pass
		else:
			class_manager.show()
			welcome_display.emit()
	else:
		print(\"JavaScriptBridge not available. Not running in a web environment.\")

func _on_vfx_button_pressed() -> void:
	vfx.visible = !vfx.visible
	if !vfx.visible:
		vfx_button_sprite.texture = load(\"res://resources/sprites/disabledvfx.png\")
	else:
		vfx_button_sprite.texture = load(\"res://resources/sprites/vfx.png\")

# --- EASTEREGG MANAGER SUBSECTION ---

@onready var easter_scenes: Array[Node] = [
	$deziangle,
	$cubigor,
	$nightcity,
	$gax
]

func show_only_node(target_node: Node) -> void:
	for node in easter_scenes:
		node.visible = (node == target_node)

func hide_all_nodes() -> void:
	for node in easter_scenes:
		node.visible = false

func show_all_nodes() -> void:
	for node in easter_scenes:
		node.visible = true

func easter_state_enabler(easter_name: String, is_3d: bool):
	easter_button_sprite.show()
	easter_button.disabled = false
	os_billboard.hide()
	textcolorchange.emit(true)
	if is_3d == true:
		background.hide()
	match easter_name:
		\"deziangle\":
			show_only_node($deziangle)
		\"cubigor\":
			show_only_node($cubigor)
		\"gax\":
			show_only_node($gax)
		\"nightcity\":
			show_only_node($nightcity)
			background.texture = load(\"res://resources/sprites/nightcity.png\")
			var stylebox = load(\"res://resources/scenes/Main_scene/Main_scene.tscn::StyleBoxFlat_yma61\")
			stylebox.bg_color = Color(0.19, 0.221, 0.306)
			panel.set(\"custom_styles/panel\", stylebox)

func easter_state_disabler():
	if background.visible == false:
		background.show()

func _on_easter_button_pressed() -> void:
	easter_state_disabler()

func update_time() -> void:
	var date_dict: Dictionary = Time.get_datetime_dict_from_system()
	time.text = \"%02d:%02d\" % [date_dict.hour, date_dict.minute]
	date.text = \"%02d/%02d/%d\" % [date_dict.day, date_dict.month, date_dict.year]

func _on_timer_timeout() -> void:
	update_time()
"

[sub_resource type="GDScript" id="GDScript_5qsst"]
resource_name = "soundmanager"
script/source = "extends AudioStreamPlayer

@onready var sfx_button_sprite: TextureRect = $\"../contents/panel/Panel/HBoxContainer/sfx_button_sprite\"
var easter_state: bool = false
var play_ambient_noise: bool = false
var sfx_enabled_texture = preload(\"res://resources/sprites/sfx.png\")
var sfx_disabled_texture = preload(\"res://resources/sprites/disabledsfx.png\")

func _ready() -> void:
	pass

func _process(_delta: float) -> void:
	if !self.playing and play_ambient_noise:
		self.play()
		sfx_button_sprite.texture = sfx_enabled_texture
	elif self.playing == true and play_ambient_noise == false:
		sfx_button_sprite.texture = sfx_disabled_texture
		self.stop()

func _on_sfx_button_pressed() -> void:
	play_ambient_noise = !play_ambient_noise


func _on_splash_screen_finished_boot() -> void:
	play_ambient_noise = true
"

