config_version=5

[application]

config/name="Varsec Fantasy OS"
config/description="Varsec OS is a fantasy Operating System made as an art project. It serves as a portfolio for his school and personal projects"
config/version="1.10: The Silent Remains"
run/main_scene="res://Files/scenes/Splash_screen/splash_screen.scn"
config/features=PackedStringArray("4.4")
run/max_fps=60
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="uid://ba6ovj2sh64g3"

[autoload]

Corec="*res://Files/Autoloads/core.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0

[display]

window/size/always_on_top=true
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Files/sprites/cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdfxr/plugin.cfg")

[file_customization]

folder_colors={
